{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bin": {
    "stack": "src/cli.js"
  },
  "bugs": "https://github.com/Shuunen/stack/issues",
  "description": "A cli to help me build web things, all in one",
  "devDependencies": {
    "@types/jsonfile": "6.0.0",
    "@types/mocha": "8.2.2",
    "chokidar": "3.5.1",
    "colorette": "1.2.2",
    "esbuild": "0.12.8",
    "jsonfile": "6.1.0",
    "npm-check-updates": "11.6.0",
    "npm-run-all": "4.1.5",
    "repo-check": "1.17.0",
    "servor": "4.0.2",
    "shuutils": "4.5.1",
    "typescript": "4.3.2",
    "xo": "0.40.2"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/Shuunen/stack",
  "keywords": [
    "cli",
    "web"
  ],
  "license": "GPL-3.0",
  "main": "dist/cli.js",
  "name": "shuunen-stack",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/stack.git"
  },
  "scripts": {
    "build": "esbuild src/cli.ts --bundle --minify --outfile=dist/cli.js --platform=node --external:node-gyp --target=node14",
    "check": "repo-check",
    "ci": "run-s build lint check",
    "lint": "xo --fix",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "npm run ci",
    "start": "node .",
    "test": "xo",
    "update": "npm-check-updates -u"
  },
  "version": "0.6.2"
}
